FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
    volScalarFieldValue zoneID 0
    volScalarFieldValue porosityIndex 0
);

regions
(
    boxToCell
    {
        box (-1 -1 0) (1 1 0.5);
        fieldValues
        (
            volScalarFieldValue alpha.water 1//1
        );
    }
    
    boxToCell
    {
        box (-0.2 -0.2 0) (0.2 0.2 0.6);
        fieldValues
        (
            volScalarFieldValue porosityIndex 1//1
        );
    }
);


