FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    {
        name OB;
        type cellZoneSet;
        action new;
        source surfaceToCell;
        sourceInfo
        {
            file "constant/triSurface/OBW1.stl";
            outsidePoints ((0 0 0));
            includeCut      true;
            includeInside   true;
            includeOutside  false;
            nearDistance    -1;
            curvature       0.9;
            useSurfaceOrientation false;
        }
    }
    
    {
        name OB;
        type cellZoneSet;
        action add;
        source surfaceToCell;
        sourceInfo
        {
            file "constant/triSurface/OBW2.stl";
            outsidePoints ((0 0 0));
            includeCut      true;
            includeInside   true;
            includeOutside  false;
            nearDistance    -1;
            curvature       0.9;
            useSurfaceOrientation false;
        }
    }
);

