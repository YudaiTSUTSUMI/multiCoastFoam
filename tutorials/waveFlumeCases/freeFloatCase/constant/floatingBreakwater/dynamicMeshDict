// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dynamicFvMesh       bulletOversetFvMesh;

solvers
{
    FBM
    {                
        mode        outerForce;
        
        objName     FBW;
        
        zoneID 1;

        bulletMotionCoeffs
        {

            patches         (FBW);
            innerDistance   100.0;
            outerDistance   101.0;
        }
    }

    inletOutlet
    {                
        mode        tracking;
        
        objName     FBW;
        
        zoneID 0;

        normal  (1 1 0);  // default      
        axis  (0 0 1);    //default

        bulletMotionCoeffs
        {

            patches         (inletOutlet);
            innerDistance   100.0;
            outerDistance   101.0;
        }
    }
}

