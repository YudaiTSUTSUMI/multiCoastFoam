SWVOFConnectors
{
	connect1
	{
		mode fixed;
		
		SWRegion       flume2D;
		VOFRegion    solitaryWavePaddle;
		
		VOFPatch   outlet;		
	}
	
	connect2
	{
		mode fixed;
		
		SWRegion     flume2D;
		VOFRegion    cylinderRegion;
		
		VOFPatch   inlet;		
	}	
}

VOFLaplacianConnectors
{
	connect1
