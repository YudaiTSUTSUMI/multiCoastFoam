(
    (-0.025  -0.025 0.05)
    (0.025   -0.025 0.05)
    (0.025    0.025 0.05)
    (-0.025   0.025 0.05)
    (-0.025  -0.025 0.2)
    (0.025   -0.025 0.2)
    (0.025    0.025 0.2)
    (-0.025   0.025 0.2)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (20 20 60) simpleGrading (1 1 1)
);

edges
(
);

